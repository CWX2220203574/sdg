{
    "tooltip": {
        "trigger": "item",
        "axisPointer": {
            "type": "shadow"
        },
        "textStyle": {
            "fontStyle": "italic"
        }
    },
    "legend": {
        "show": false,
        "top": "4%",
        "left": "center",
        "data": [
            "客户细分",
            "绿色营销",
            "并购"
        ]
    },
    "series": [
        {
            "name": "Access From",
            "type": "pie",
            "radius": [
                "75%",
                "95%"
            ],
            "center": [
                "30%",
                "70%"
            ],
            "startAngle": 90,
            "endAngle": 360,
            "data": [
                {
                    "value": 300,
                    "name": "客户细分"
                },
                {
                    "value": 600,
                    "name": "绿色营销"
                },
                {
                    "value": 900,
                    "name": "并购"
                }
            ],
            "itemStyle": {
                "borderColor": "#ccc",
                "borderWidth": 4,
                "borderRadius": 10
            },
            "label": {
                "show": true,
                "position": "outside",
                "formatter": "{b}: {d}%",
                "fontSize": 18,
                "color": "#666",
                "distance": 15
            },
            "emphasis": {
                "itemStyle": {
                    "borderColor": "#ff9800"
                },
                "label": {
                    "fontSize": 20
                }
            }
        }
    ],
    "backgroundColor": "#eaeaea",
    "title": {
        "text": "方形饼图示例",
        "left": "center",
        "top": "1%",
        "textStyle": {
            "fontSize": 20,
            "color": "#333"
        }
    }
}